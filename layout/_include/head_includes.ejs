<link rel="shortcut icon" type="image/x-icon" href="<%- url_for("avatar.png") %>">

<%- include("cdn.ejs") %>
<link rel="stylesheet" href="<%= this.cdn.libs.css.bootstrap %>" crossorigin="anonymous">
<link rel="stylesheet" href="<%= this.cdn.libs.css.fancybox %>" crossorigin="anonymous">
<link rel="stylesheet" href="<%= this.cdn.libs.css.katex %>" crossorigin="anonymous">
<link rel="stylesheet" href="<%- url_for(`css/journal.css?${Math.round(Math.random() * 99999999)}`) %>">
<% if (theme.favicon || config.favicon){ %>
<link rel="icon" href="<%- config.favicon ? config.favicon : theme.favicon %>">
<% } %>
<% if (theme.hljsTheme) { %>
<link rel="stylesheet" href="<%= ["/", "http:", "https:"].some(s => theme.hljsTheme.startsWith(s)) ? theme.hljsTheme : (this.cdn.libs.css.hljs + theme.hljsTheme + this.cdn.libs.css.hljsSuffix) %>" crossorigin="anonymous">
<% } %>
<link rel="stylesheet" href="<%= this.cdn.fonts %>/css2?family=Noto+Sans+SC:wght@300%3B400%3B600&family=Source+Sans+Pro:wght@400%3B600&family=Lato:wght@400%3B500&family=Hind+Vadodara:wght@300%3B400&family=Montserrat&family=Fira+Code&family=Material+Icons&display=block" crossorigin="anonymous">

<% if (theme.googleAnalytics) { %>
<script async src="https://www.googletagmanager.com/gtag/js"></script>
<script>window.dataLayer=window.dataLayer||[];function gtag(){dataLayer.push(arguments);}gtag('js',new Date());gtag('config', '<%= theme.googleAnalytics %>');</script>
<% } %>
<% if (theme.plausible) { %>
<script async data-domain="<%= theme.plausible.domain %>" src="<%= theme.plausible.script %>"></script>
<% } %>
