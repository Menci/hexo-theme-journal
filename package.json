{
    "name": "hexo-theme-journal",
    "version": "2.0.3",
    "description": "Moments got piled up.",
    "main": "index.js",
    "scripts": {
        "test": "npm run build",
        "build": "node-sass source/scss/journal.scss --output-style compressed -o source/css && postcss source/css/journal.css -u autoprefixer -r --no-map",
        "watch": "node-sass -w source/scss -o source/css"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/SumiMakito/hexo-theme-journal"
    },
    "keywords": [
        "hexo",
        "theme",
        "journal"
    ],
    "author": "Makito <sumimakito@hotmail.com> (https://makito.cc/)",
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/SumiMakito/hexo-theme-journal/issues"
    },
    "homepage": "https://github.com/SumiMakito/hexo-theme-journal/README.md",
    "devDependencies": {},
    "dependencies": {
        "autoprefixer": "^10.4.2",
        "node-sass": "^7.0.1",
        "postcss": "^8.4.5",
        "postcss-cli": "^9.1.0",
        "scss-compile": "^0.1.7"
    }
}
